{
  "id": "ctzn.network/methods/community-delete-ban",
  "title": "Delete ban method",
  "description": "Delete a ban-record from a community.",
  "type": "method",
  "parameters": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["bannedUser"],
    "properties": {
      "bannedUser": {
        "type": "object",
        "required": ["dbUrl", "userId"],
        "properties": {
          "dbUrl": {
            "type": "string",
            "format": "uri"
          },
          "userId": {
            "type": "string",
            "pattern": ".+@.+"
          }
        }
      }
    }
  }
}