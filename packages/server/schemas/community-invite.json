{
  "id": "ctzn.network/community-invite",
  "title": "Community invite",
  "description": "An invite to join a community.",
  "type": "json-table",
  "keyTemplate": [{"type": "json-pointer", "value": "/invitedUser/dbKey"}],
  "definition": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["invitedUser", "createdBy", "createdAt"],
    "properties": {
      "invitedUser": {
        "type": "object",
        "required": ["dbKey"],
        "properties": {
          "dbKey": {
            "type": "string",
            "pattern": "^[0-9a-f]{64}$"
          }
        }
      },
      "createdBy": {
        "type": "object",
        "required": ["dbKey"],
        "properties": {
          "dbKey": {
            "type": "string",
            "pattern": "^[0-9a-f]{64}$"
          }
        }
      },
      "createdAt": {
        "type": "string",
        "format": "date-time"
      }
    }
  }
}